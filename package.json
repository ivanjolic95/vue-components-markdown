{
  "name": "vue-components-markdown",
  "version": "0.0.2",
  "license": "MIT",
  "description": "Runtime markdown processor with support for injecting your custom Vue components into your markdown text.",
  "author": "Ivan Jolic <ivan.jolic1995@gmail.com>",
  "main": "dist/vue-components-markdown.common.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/docs/main.js",
    "build": "vue-cli-service build --target lib --name vue-components-markdown src/index.js",
    "build:docs": "vue-cli-service build --name vue-components-markdown src/docs/main.js --dest docs",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit --passWithNoTests"
  },
  "dependencies": {
    "@vue/test-utils": "^1.2.2",
    "babel-runtime": "^6.26.0",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-markdown": "^2.2.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^6.0.1",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
