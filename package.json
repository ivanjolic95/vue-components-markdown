{
  "name": "vue-components-markdown",
  "version": "0.0.3",
  "license": "MIT",
  "description": "Runtime markdown processor with support for injecting your custom Vue components into your markdown text.",
  "author": "Ivan Jolic <ivan.jolic1995@gmail.com>",
  "main": "dist/vue-components-markdown.common.js",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/docs/main.js",
    "build": "vue-cli-service build --target lib --name vue-components-markdown src/index.js",
    "build:docs": "vue-cli-service build --name vue-components-markdown src/docs/main.js --dest docs",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit --passWithNoTests"
  },
  "dependencies": {
    "@vue/test-utils": "^2.0.0",
    "babel-runtime": "^6.26.0",
    "core-js": "^3.6.5",
    "vue": "3",
    "vue3-markdown-it": "^1.0.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.8",
    "@vue/cli-plugin-babel": "^5.0.0",
    "@vue/cli-plugin-eslint": "^5.0.0",
    "@vue/cli-plugin-unit-jest": "^5.0.0",
    "@vue/cli-service": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^9.14.1",
    "vue-template-compiler": "^2.6.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
